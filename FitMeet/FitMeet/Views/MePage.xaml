<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FitMeet.Views.MePage"
             xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
             prism:ViewModelLocator.AutowireViewModel="True" Title="{Binding Title}">

	<ContentPage.ToolbarItems>
		<ToolbarItem Name="Edit" Icon="edit_logo.png" 
		             Command="{Binding NavigateCommand}" 
		             CommandParameter="app:///MainPage/NavigationPage/MainTabbedPage/MePage/SettingPage" ></ToolbarItem>
	</ContentPage.ToolbarItems>
	<ContentPage.Content>
        <StackLayout>
            <Label Text="Welcome to Xamarin Forms!" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>